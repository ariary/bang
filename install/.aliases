alias firefox='(&>/dev/null firefox &)'
alias hgrep='history | grep'
alias screenshot='flameshot gui'
alias ll='ls -al'
alias tmpdir='cd $(mktemp -d)'
alias b64='base64'
alias b64d='base64 -d'
eb64(){echo "${1}" | base64}
eb64d(){echo "${1}" | base64 -d}
alias envgrep='env | grep'
alias cheat='f(){ curl -s "cheat.sh/$1";  unset -f f; }; f'
alias dockit='docker run --rm -it -v "$PWD":/host --net=host'
alias docker-stopall='docker stop $(docker ps -q)'
